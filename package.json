{
  "name": "wrap-royale",
  "version": "1.5.0",
  "description": "A promise based, feature-rich library for requestion resources from Supercell's official Clash Royale API, written in TypeScript.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "tslint --project tsconfig.json --format stylish src/**/*.ts",
    "format": "prettier --write \"**/*.ts\"",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weeco/wrap-royale.git"
  },
  "keywords": [
    "clash",
    "royale",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "Weeco <weeco91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weeco/wrap-royale/issues"
  },
  "homepage": "https://github.com/weeco/wrap-royale#readme",
  "dependencies": {
    "bases": "^0.2.1",
    "long": "^4.0.0",
    "moment": "^2.22.2",
    "reflect-metadata": "^0.1.12",
    "slug": "^0.9.1",
    "wrap-royale-core": "^1.3.0"
  },
  "devDependencies": {
    "@types/bases": "^0.2.2",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-json-schema": "^1.4.3",
    "@types/dotenv": "^4.0.3",
    "@types/joi": "^13.4.0",
    "@types/long": "^4.0.0",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.5.2",
    "@types/slug": "^0.9.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-json-schema": "^1.5.0",
    "dotenv": "^6.0.0",
    "joi": "^13.4.0",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "tcs": "^10.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.1.0",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
